pipeline {
    agent any
    stages{
        stage('sonarqube scan'){
            steps{
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=mumba2023_Geolocation-a'
            }
        }
    }
    tools{
        maven 'M2_HOME'
    }  
    stages {
        stage('Maven commands') {
            steps {
                script {
                    sh 'mvn clean test compile install package'
                }
            }
        }
    }
}
